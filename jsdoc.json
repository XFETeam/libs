{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".js$",
    "exclude": [
      "./node_modules",
      "./docs",
      "./src/models",
      "./src/error"
    ]
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-escape-at"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "docdash": {
    "search": true
  }
}
