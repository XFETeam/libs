{
  "name": "@xfe-team/universal-auth",
  "version": "0.0.1",
  "author": "liyihong",
  "description": "西山居多端通用登录",
  "main": "lib/@xfe-team/universal-auth.js",
  "scripts": {
    "start": "webpack --progress --colors --watch --env dev",
    "build": "rimraf lib && webpack --env development && webpack --env production && git add lib",
    "prepublishOnly": "npm run -s build",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "release": "npm run build && release-it patch",
    "release:dry-run": "release-it minor --dry-run",
    "deploy": "npm run build"
  },
  "keywords": [
    "xfe",
    "universal-auth"
  ],
  "files": [
    "lib"
  ],
  "license": "MIT",
  "homepage": "https://github.com/XFETeam/libs/tree/universal-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/XFETeam/libs/tree/universal-auth"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "babel-eslint": "8.0.3",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "5.1.0",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "chai": "4.1.2",
    "cross-env": "5.2.0",
    "eslint": "5.0.1",
    "eslint-loader": "2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "4.0.1",
    "nyc": "13.1.0",
    "release-it": "12.4.3",
    "rimraf": "3.0.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.12.2",
    "webpack-cli": "3.0.8",
    "yargs": "10.0.3",
    "raw-loader": "4.0.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "detect-mobile-device": "0.0.4",
    "jsonp-promise": "0.1.2"
  }
}
