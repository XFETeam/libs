{
  "name": "@xfe-team/json-monaco-editor",
  "version": "0.0.2",
  "description": "json 在线编辑工具, 通过 iframe 进行外部通信",
  "author": "xfe-team",
  "license": "MIT",
  "repository": "XFETeam/libs",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
